        "\treturn y\n",
        "\n",
        "\n",
        "#high-pass filter\n",
        "def hpf(x):\n",
        "\ty = x.copy()\n",
        "\t\n",
        "\tfor n in x.index:\n",
        "\t\tif(n < 32):\n",
